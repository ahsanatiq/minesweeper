{
    "name": "ahsanatiq/minesweeper",
    "description": "minesweeper game implementation in PHP console",
    "keywords": [
        "minesweeper",
        "php72",
        "console",
        "symfony",
        "climate",
        "phpunit",
        "tests"
    ],
    "homepage": "http://github.com/ahsanatiq/minesweeper",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahsan Atiq",
            "email": "me@ahsanatiq.info",
            "homepage": "https://www.ahsanatiq.info/"
        }
    ],
    "require": {
        "php": "~7.2",
        "illuminate/config": "^5.8",
        "illuminate/container": "^5.8",
        "league/climate": "^3.5",
        "monolog/monolog": "^1.24",
        "symfony/console": "^4.3",
        "symfony/finder": "^4.3",
        "vlucas/phpdotenv": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/app/",
            "Minesweeper\\": "src/minesweeper/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/app/helpers.php"
        ]
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
